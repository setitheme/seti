{
  "name": "@setitheme/core",
  "version": "1.0.0",
  "description": "Official icon pack for the Seti theme",
  "author": "Jesse Weed <code@jesseweed.io> (http://jesseweed.io)",
  "license": "MIT",
  "homepage": "https://setitheme.org",
  "main": "build/seti.core.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/setitheme/core.git",
    "directory": "packages/react-dom"
  },
  "bugs": {
    "url": "https://github.com/setitheme/core/issues"
  },
  "engines": {
    "node": ">15.0.0",
    "npm": ">7.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx}\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "setitheme",
    "setiui",
    "seti theme",
    "seti core",
    "seti config"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "chai": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
